# prepare some data
from bokeh.io import output_file, show
from bokeh.plotting import figure
from bokeh.models import ColumnDataSource
import pandas as pd
import bokeh


df = pd.read_csv("world-happiness-report-2021.csv")

Happiness = df["Ladder score"]
Country = df["Country name"]
GDP = df["Logged GDP per capita"]

p = figure(y_range="Country", plot_width=800, plot_height=600, title='Happiness Observation (Country)', y_axis_label="Country", x_axis_label="Ladder score")
p.hbar(
    y=Country,
    right=Happiness,
    left=0,
    height=0.4,
    color="orange",
    fill_alpha=0.5
)

output_file("index.html")